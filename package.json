{
  "name": "dg",
  "version": "1.0.0",
  "description": "WP theme starter pack",
  "scripts": {
    "styl": "stylus -w -u autoprefixer-stylus --with \"{ browsers: ['last 2 versions'] }\" --sourcemap-inline css/styl/style.styl -o css/style.css",
    "serve": "lite-server",
    "dev": "npm-run-all --parallel styl serve"
  },
  "author": "Dalibor Gogic <mail@daliborgogic.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-stylus": "^0.13.0",
    "http-proxy-middleware": "^0.17.4",
    "lite-server": "^2.3.0",
    "micro": "github:daliborgogic/micro-css",
    "npm-run-all": "^4.0.2",
    "stylus": "^0.54.5"
  }
}
